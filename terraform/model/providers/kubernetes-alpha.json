{
  ".schema_version": "2",
  ".sdk_type": "terraform-tfplugin5",
  "name": "kubernetes-alpha",
  "type": "provider",
  "version": "v0.2.1",
  "provider": {
    "client_certificate": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "client_key": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "cluster_ca_certificate": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "config_context": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "config_context_cluster": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "config_context_user": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "config_path": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "exec": {
      "Type": "Object({ api_version=String args=List(String) command=String env=Map(String) })",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "host": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "insecure": {
      "Type": "Bool",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "password": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "server_side_planning": {
      "Type": "Bool",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "token": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    },
    "username": {
      "Type": "String",
      "Optional": true,
      "ConfigImplicitMode": "Attr"
    }
  },
  "resources": {
    "kubernetes_manifest": {
      "manifest": {
        "Type": "Any",
        "Required": true,
        "Description": "A Kubernetes manifest describing the desired state of the resource in HCL format.",
        "ConfigImplicitMode": "Attr"
      },
      "object": {
        "Type": "Any",
        "Optional": true,
        "Description": "The resulting resource state, as returned by the API server after applying the desired state from `manifest`.",
        "Computed": true,
        "ConfigImplicitMode": "Attr"
      },
      "wait_for": {
        "Type": "Object({ fields=Map(String) })",
        "Optional": true,
        "Description": "A map of attribute paths and desired patterns to be matched. After each apply the provider will wait for all attributes listed here to reach a value that matches the desired pattern.",
        "ConfigImplicitMode": "Attr"
      }
    }
  },
  "data-sources": {}
}